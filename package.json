{
  "name": "webpack-lighthouse-template",
  "version": "1.0.0",
  "author": "Ethan Soo Hon",
  "description": "Simple webpack 4 template for optimizing the performance of static sites in production",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "build": " npm run clean && webpack --config webpack.prod.js  --stats-children",
    "start-dev": "webpack serve --config webpack.dev.js",
    "start-mobile-dev": "webpack serve --host 0.0.0.0 --config webpack.dev.js",
    "convert-webp": "node buildImages.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanny2/webpack-lighthouse100-template"
  },
  "keywords": [
    "webpack",
    "template",
    "lighthouse 100",
    "perfect score"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ethanny2/webpack-lighthouse100-template/issues"
  },
  "homepage": "https://github.com/ethanny2/webpack-lighthouse100-template",
